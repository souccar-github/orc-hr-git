IF (select Count(authElements.AuthorizableElementId) from Souccar_Security_User _user left join Souccar_Security_UserRole _user_role on _user.Id = _user_role.User_id 
left join Souccar_Security_AuthorizableElementRole authElements on _user_role.Role_id = authElements.Role_id 
where _user.Id = @currentUserId and
 authElements.AuthorizableElementType = 9) &gt; 0
BEGIN 
SELECT DISTINCT Employee.Id, Employee.FirstName + ' ' + ISNULL(Employee.FatherName, '') + ' ' + Employee.LastName AS FullName
FROM            Employee
inner join PrimaryCard on PrimaryCard.Employee_id = Employee.Id
inner join MonthlyCard on MonthlyCard.PrimaryCard_id = PrimaryCard.Id
inner join month on month.Id = MonthlyCard.Month_id 
where
Employee.IsVertualDeleted = 0 and
month.IsVertualDeleted = 0 and 
MonthlyCard.IsVertualDeleted = 0 and 
primarycard.IsVertualDeleted =0 and
 month.id = @month and
 Employee.Id in (select distinct _emp.Id from  Souccar_Security_User _user left join Employee _emp on _user.Username = CONVERT(varchar(10), _emp.Id)
left join HistoryOfPosition _assign on _emp.Id = _assign.Employee_id 
left join Position _position on _assign.Position_Id = _position.Id 
left join JobDescription _jobDesc on _position.JobDescription_id = _jobDesc.Id 
left join Node _node on _jobDesc.Node_id = _node.Id 
where _user.IsVertualDeleted = 0 and 
_emp.IsVertualDeleted = 0 and
_assign.IsVertualDeleted = 0 and
_position.IsVertualDeleted = 0 and
_jobDesc.IsVertualDeleted = 0 and
_node.IsVertualDeleted = 0  and
_node.Id in (select authElements.AuthorizableElementId from Souccar_Security_User _user left join Souccar_Security_UserRole _user_role on _user.Id = _user_role.User_id 
left join Souccar_Security_AuthorizableElementRole authElements on _user_role.Role_id = authElements.Role_id 
where _user.Id = @currentUserId and authElements.AuthorizableElementType = 9)) AND (Employee.FirstName + ' ' + ISNULL(Employee.FatherName, N'') 
                         + ' ' + Employee.LastName LIKE '%' + ISNULL(@FilterEmployee, N'') + '%')
ORDER BY FullName
END
ELSE
BEGIN

SELECT DISTINCT Employee.Id, Employee.FirstName + ' ' + ISNULL(Employee.FatherName, '') + ' ' + Employee.LastName AS FullName
FROM            Employee
inner join PrimaryCard on PrimaryCard.Employee_id = Employee.Id
inner join MonthlyCard on MonthlyCard.PrimaryCard_id = PrimaryCard.Id
inner join month on month.Id = MonthlyCard.Month_id
where
Employee.IsVertualDeleted = 0 and
month.IsVertualDeleted = 0 and 
MonthlyCard.IsVertualDeleted = 0 and 
primarycard.IsVertualDeleted =0 and
 month.id = @month 

AND (Employee.FirstName + ' ' + ISNULL(Employee.FatherName, N'') 
                         + ' ' + Employee.LastName LIKE '%' + ISNULL(@FilterEmployee, N'') + '%')
ORDER BY FullName

END</CommandText></Query><Fields><Field Name="Id"><DataField>Id</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field></Fields></DataSet></DataSets><ReportParameters><ReportParameter Name="FilterMonth"><DataType>String</DataType><DefaultValue><Values><Value /></Values></DefaultValue><AllowBlank>true</AllowBlank><Prompt>بحث</Prompt></ReportParameter><ReportParameter Name="month"><DataType>Integer</DataType><Prompt>الشهر</Prompt><ValidValues><DataSetReference><DataSetName>MonthDataSet</DataSetName><ValueField>Id</ValueField><LabelField>Name</LabelField></DataSetReference></ValidValues></ReportParameter><ReportParameter Name="FilterEmployee"><DataType>String</DataType><DefaultValue><Values><Value /></Values></DefaultValue><AllowBlank>true</AllowBlank><Prompt>بحث</Prompt></ReportParameter><ReportParameter Name="currentUserId"><DataType>Integer</DataType><DefaultValue><Values><Value>8410</Value></Values></DefaultValue><Prompt>currentUserId</Prompt><Hidden>true</Hidden></ReportParameter><ReportParameter Name="employee"><DataType>Integer</DataType><Prompt>الموظف</Prompt><ValidValues><DataSetReference><DataSetName>EmployeeDataSet</DataSetName><ValueField>Id</ValueField><LabelField>FullName</LabelField></DataSetReference></ValidValues></ReportParameter></ReportParameters><Language>en-US</Language><ConsumeContainerWhitespace>true</ConsumeContainerWhitespace><rd:ReportUnitType>Cm</rd:ReportUnitType><rd:ReportID>eaffd80e-ad0a-4976-9d4a-08aba77b1e58</rd:ReportID></Report>