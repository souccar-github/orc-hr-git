og=HR-MAIN-DB</ConnectString></ConnectionProperties><rd:SecurityType>DataBase</rd:SecurityType><rd:DataSourceID>0f5f43df-b9b8-4d68-8c9a-9c12be59019d</rd:DataSourceID></DataSource></DataSources><DataSets><DataSet Name="BasicInfoDataSet"><Query><DataSourceName>SharedDataSource</DataSourceName><QueryParameters><QueryParameter Name="@month"><Value>=Parameters!month.Value</Value></QueryParameter><QueryParameter Name="@employee"><Value>=Parameters!employee.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        
MonthlyCard.Id AS MonthlyCardId, 
Employee.Code AS EmployeeCode, 
Employee.FirstName + ' ' + Employee.FatherName+ ' ' + Employee.LastName AS FullName, 
Month.Name AS MonthName, 
Month.Id AS MonthId, 
MonthType,
Date, MonthNumber,
MonthlyCard.BasicSalary, 
MonthlyCard.Salary, 
MonthlyCard.BenefitSalary,
MonthlyCard.TaxableAmount,
MonthlyCard.WorkDays,
MonthlyCard.FinalMonthSalary,
MonthlyCard.TotalEmployeeDeserved As TotalEmployeeDeserved,
MonthlyCard.EmployeeDeserved As EmployeeDeserved,
Employee.Id AS EmployeeId
FROM            
Month 
INNER JOIN MonthlyCard ON MonthlyCard.Month_id = Month.Id 
INNER JOIN PrimaryCard ON PrimaryCard.Id = MonthlyCard.PrimaryCard_id 
INNER JOIN Employee ON Employee.Id = PrimaryCard.Employee_id 
WHERE
 MonthlyCard.ActualMonthSalary&gt;0 AND        
(MonthlyCard.IsVertualDeleted = 0) AND 
(Month.IsVertualDeleted = 0) AND 
(PrimaryCard.IsVertualDeleted = 0) AND 
(Employee.IsVertualDeleted = 0)  AND MonthlyCard.Id in (SELECT  top 1
MonthlyCard.Id AS MonthlyCardId
FROM            
Month 
INNER JOIN MonthlyCard ON MonthlyCard.Month_id = Month.Id 
INNER JOIN PrimaryCard ON PrimaryCard.Id = MonthlyCard.PrimaryCard_id 
INNER JOIN Employee ON Employee.Id = PrimaryCard.Employee_id 
WHERE        
(MonthlyCard.IsVertualDeleted = 0) AND 
(Month.IsVertualDeleted = 0) AND 
(PrimaryCard.IsVertualDeleted = 0) AND 
(Employee.IsVertualDeleted = 0) AND 
Month.Id = @month AND
Employee.Id = @employee)</CommandText></Query><Fields><Field Name="MonthlyCardId"><DataField>MonthlyCardId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="EmployeeCode"><DataField>EmployeeCode</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="FullName"><DataField>FullName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="MonthName"><DataField>MonthName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="MonthId"><DataField>MonthId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="MonthType"><DataField>MonthType</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="Date"><DataField>Date</DataField><rd:TypeName>System.DateTime</rd:TypeName></Field><Field Name="MonthNumber"><DataField>MonthNumber</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="BasicSalary"><DataField>BasicSalary</DataField><rd:TypeName>System.Single</rd:TypeName></Field><Field Name="Salary"><DataField>Salary</DataField><rd:TypeName>System.Single</rd:TypeName></Field><Field Name="BenefitSalary"><DataField>BenefitSalary</DataField><rd:TypeName>System.Single</rd:TypeName></Field><Field Name="TaxableAmount"><DataField>TaxableAmount</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="WorkDays"><DataField>WorkDays</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="FinalMonthSalary"><DataField>FinalMonthSalary</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="TotalEmployeeDeserved"><DataField>TotalEmployeeDeserved</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="EmployeeDeserved"><DataField>EmployeeDeserved</DataField><rd:TypeName>System.Double</rd:TypeName></Field><Field Name="EmployeeId"><DataField>EmployeeId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field></Fields></DataSet><DataSet Name="MonthlyEmployeeDeductionDataSet"><Query><DataSourceName>SharedDataSource</DataSourceName><QueryParameters><QueryParameter Name="@month"><Value>=Parameters!month.Value</Value></QueryParameter><QueryParameter Name="@employee"><Value>=Parameters!employee.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        MonthlyEmployeeDeduction.MonthlyCard_id AS MonthlyCardId, DeductionCard.Name AS DeductionName, 
                         CASE WHEN Month.MonthType = 0 THEN MonthlyEmployeeDeduction.FinalValue ELSE MonthlyEmployeeDeduction.FinalValue - MonthlyEmployeeDeduction.InitialValue END AS DeductionValue
FROM            Month INNER JOIN
                         MonthlyCard ON MonthlyCard.Month_id = Month.Id INNER JOIN
                         MonthlyEmployeeDeduction ON MonthlyEmployeeDeduction.MonthlyCard_id = MonthlyCard.Id INNER JOIN
                         DeductionCard ON DeductionCard.Id = MonthlyEmployeeDeduction.DeductionCard_id
WHERE        (MonthlyEmployeeDeduction.DeductionCard_id NOT IN
                             ((SELECT        HealthyLeaveDeduction_id
                                 FROM            GeneralOption),
                             (SELECT        UnpaidMaternityLeaveDeduction_id
                                FROM            GeneralOption AS GeneralOption_6),
                             (SELECT        MaternityLeaveDeduction_id
                                FROM            GeneralOption AS GeneralOption_5),
                             (SELECT        AdministrativeLeaveDeduction_id
                                FROM            GeneralOption AS GeneralOption_4),
                             (SELECT        UnpaidLeaveDeduction_id
                                FROM            GeneralOption AS GeneralOption_3),
                             (SELECT        OtherLeaveDeduction_id
                                FROM            GeneralOption AS GeneralOption_2),
                             (SELECT        UnspecifiedUnpaidLeaveDeduction_id
                                FROM            GeneralOption AS GeneralOption_1))) AND (Month.IsVertualDeleted = 0) AND (MonthlyEmployeeDeduction.IsVertualDeleted = 0) AND (MonthlyCard.IsVertualDeleted = 0) AND (DeductionCard.IsVertualDeleted = 0) 
                         AND (MonthlyCard.Id in (SELECT  top 1
MonthlyCard.Id AS MonthlyCardId
FROM            
Month 
INNER JOIN MonthlyCard ON MonthlyCard.Month_id = Month.Id 
INNER JOIN PrimaryCard ON PrimaryCard.Id = MonthlyCard.PrimaryCard_id 
INNER JOIN Employee ON Employee.Id = PrimaryCard.Employee_id 
WHERE        
(MonthlyCard.IsVertualDeleted = 0) AND 
(Month.IsVertualDeleted = 0) AND 
(PrimaryCard.IsVertualDeleted = 0) AND 
(Employee.IsVertualDeleted = 0) AND 
Month.Id = @month AND
Employee.Id = @employee))</CommandText></Query><Fields><Field Name="MonthlyCardId"><DataField>MonthlyCardId</DataField><rd:TypeName>System.Int32</rd:TypeName></Field><Field Name="DeductionName"><DataField>DeductionName</DataField><rd:TypeName>System.String</rd:TypeName></Field><Field Name="DeductionValue"><DataField>DeductionValue</DataField><rd:TypeName>System.Double</rd:TypeName></Field></Fields></DataSet><DataSet Name="LoanDataSet"><Query><DataSourceName>SharedDataSource</DataSourceName><QueryParameters><QueryParameter Name="@month"><Value>=Parameters!month.Value</Value></QueryParameter><QueryParameter Name="@employee"><Value>=Parameters!employee.Value</Value></QueryParameter></QueryParameters><CommandText>SELECT        LoanPayment.MonthlyCard_id AS MonthlyCardId, DonorLoan.Name AS DonorLoanName, LoanPayment.PaymentValue, EmployeeLoan.TotalAmountOfLoan - (EmployeeLoan.PrePayed +
                             (SELECT        ISNULL(SUM(PaymentValue), 0) AS Expr1
                                FROM            LoanPayment AS N
                                WHERE        (EmployeeLoan_id = EmployeeLoan.Id) AND (IsVertualDeleted = 0))) AS RemainingValue, LoanPayment.RemainingValueAfterPaymentValue
FROM            MonthlyCard INNER JOIN
                         LoanPayment ON LoanPayment.MonthlyCard_id = MonthlyCard.Id AND LoanPayment.IsExternalPayment = 0 INNER JOIN
